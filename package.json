{
  "name": "decanter",
  "description": "Pattern Library and CSS Framework.",
  "author": "",
  "version": "6.2.18",
  "homepage": "https://github.com/SU-SWS/decanter",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=development",
    "publish": "webpack --mode=production",
    "watch": "webpack --mode=development --watch --config developer/webpack.dev.config.js",
    "start": "webpack-dev-server --mode=development --open --config developer/webpack.dev.config.js",
    "dev": "webpack --mode=development --config developer/webpack.dev.config.js",
    "eslint": "eslint -c .eslintrc 'core/src/'",
    "sasslint": "sass-lint -c .sass-lint.yml '**/*.scss' -v -q -i 'vendor/**/*.scss, node_modules/**/*.scss, kss/builder/decanter/**/*.scss'",
    "launch": "node launch-preview.js",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "keywords": [
    "css",
    "scss",
    "styleguide",
    "Stanford"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SU-SWS/decanter.git"
  },
  "bugs": {
    "url": "https://github.com/SU-SWS/decanter/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15",
    "bourbon": "^6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "auto-changelog": "^2.3.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "caniuse-lite": "^1.0",
    "css-loader": "^6.8.1",
    "custom-event-polyfill": "^1.0",
    "eslint": "^8.46.0",
    "extra-watch-webpack-plugin": "^1.0",
    "filemanager-webpack-plugin": "^8.0",
    "html-webpack-plugin": "^5.5",
    "jsdoc": "^4.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "node-pre-gyp": "^0.17.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.0",
    "raw-loader": "^4.0.1",
    "sass-lint": "^1.13",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "twig-html-loader": "^0.1.9",
    "webpack": "^5.88.2",
    "webpack-assets-manifest": "^5.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-manifest-plugin": "^5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
