{
  "name": "decanter",
  "description": "Collection of SCSS and CSS patterns to include in any Stanford project.",
  "author": "",
  "version": "5.0.0",
  "homepage": "https://github.com/SU-SWS/decanter",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=development",
    "postbuild": "npm run styleguide",
    "watch": "webpack --mode=development --watch",
    "dist": "webpack --mode=production",
    "styleguide": "kss --config=kss/kss-config.json",
    "poststyleguide": "rm -rf styleguide/img && mkdir styleguide/img && cp img-placeholders/* styleguide/img",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "css",
    "scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SU-SWS/decanter.git"
  },
  "bugs": {
    "url": "https://github.com/SU-SWS/decanter/issues"
  },
  "dependencies": {
    "bourbon": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "acorn": "^6.1.1",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "kss": "^3.0.0-beta.25",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "twig": "^1.13.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-command": "^0.4.2",
    "webpack-copy-on-build-plugin": "^1.0.4",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
