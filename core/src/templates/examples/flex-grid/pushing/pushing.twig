<div>
    <section class="flex-container centered-container">
      <div class="flex-item flex-lg-2-of-12 flex-xl-2-of-12 flex-2xl-2-of-12">
        {{ pushFirst }}
      </div>

      <div class="flex-item flex-lg-4-of-12 flex-xl-4-of-12 flex-2xl-4-of-12">
        {{ pushSecond }}
      </div>

      <div class="flex-item flex-lg-4-of-12 flex-xl-4-of-12 flex-2xl-4-of-12">
        {{ pushThird }}
      </div>

      <div class="flex-item flex-lg-2-of-12 flex-xl-2-of-12 flex-push-xs-5">
        {{ pushFourth}}
      </div>
    </section>
</div>

<figure class="highlight">
  <pre>
